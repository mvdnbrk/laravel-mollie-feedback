{
    "name": "mvdnbrk/laravel-mollie-feedback",
    "description": "Handle webhooks sent from Mollie",
    "keywords": [
        "laravel",
        "mollie",
        "feedback",
        "webhook"
    ],
    "homepage": "https://github.com/mvdnbrk/laravel-mollie-feedback",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.1",
        "illuminate/http": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\MollieFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\MollieFeedback\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvdnbrk\\MollieFeedback\\MollieFeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
